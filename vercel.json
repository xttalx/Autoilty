{
  "rewrites": [
    {
      "source": "/product",
      "destination": "/product.html"
    },
    {
      "source": "/cart",
      "destination": "/cart.html"
    },
    {
      "source": "/create-listing",
      "destination": "/create-listing.html"
    },
    {
      "source": "/marketplace",
      "destination": "/marketplace.html"
    },
    {
      "source": "/my-postings",
      "destination": "/my-postings.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/register",
      "destination": "/register.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "trailingSlash": false
}

